    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "instantsearch.js": "^4.66.0",
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18",
    "react-instantsearch": "^7.7.0",
    "react-instantsearch-router-nextjs": "^7.7.0",
    "sass": "^1.71.1",
    "typesense": "^1.7.2",
    "typesense-instantsearch-adapter": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
